{
	"name": "frcsplinecreatorextension",
	"displayName": "FRC Spline Creator Extension",
	"description": "Graphical spline creator for FRC team 2848",
	"version": "0.9.4",
	"publisher": "QJjs",
	"engines": {
		"vscode": "^1.27.0"
	},
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/AllSparks2848/FRCSplineCreatorExtension"
	},
	"bugs": {
		"url": "https://github.com/AllSparks2848/FRCSplineCreatorExtension/issues"
	},
	"activationEvents": [
		"onCommand:splineEditor.launch"
	],
	"main": "./out/extension",
	"contributes": {
		"commands": [
			{
				"command": "splineEditor.launch",
				"title": "Spline Editor: Launch"
			},
			{
				"command": "splineEditor.export.xml",
				"title": "Spline Editor: Export as XML"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^2.6.1",
		"vscode": "^1.1.21",
		"vsce": "^1.51.0",
		"tslint": "^5.8.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	}
}
